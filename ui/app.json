{
  "expo": {
    "name": "homemade",
    "slug": "homemade",
    "privacy": "unlisted",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/fridge_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/fridge_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.homemade.homemadeapp",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Give homemade permission to save photos",
        "NSCameraUsageDescription": "Give homemade permission to access your camera",
        "NSMicrophoneUsageDescription": "Give homemade permission to use your microphone"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
